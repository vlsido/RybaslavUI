{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
  "strictNullChecks": true,
  "exactOptionalPropertyTypes": true,
  "noFallthroughCasesInSwitch": true,
  "jsx": "react-jsx",
  "strict": true,
  "paths": {
    "@/*": [
      "./*"
    ]
  },
  },
  "jsx": "react-native",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}

